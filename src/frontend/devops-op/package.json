    "jest": "^27.1.0",
    "mockjs": "1.0.1-beta3",
    "runjs": "4.3.2",
    "serve-static": "1.13.2",
    "svgo": "1.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
